<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://Metro.Services.Babylon/" targetNamespace="http://Metro.Services.Babylon/" version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">
