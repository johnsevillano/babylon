<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true"/>
  </system.web>
  <!-- When deploying the service library project, the content of the config file must be added to the host's 
  app.config file. System.Configuration does not support config files for libraries. -->
  <system.serviceModel>
    <services>
      <service behaviorConfiguration="BabelServiceBehavior" name="Babel.Services.ProfileService">
        <endpoint address="" binding="wsHttpBinding" bindingNamespace="http://babylon.com/services" contract="Babel.Services.IProfileService"/>
        <endpoint address="basic" binding="basicHttpBinding" bindingNamespace="http://babylon.com/services" contract="Babel.Services.IProfileService"/>
        <endpoint address="mex" binding="mexHttpBinding" bindingNamespace="http://babylon.com/services" contract="IMetadataExchange"/>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/Babel.Service.Profile"/>
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="BabelServiceBehavior" name="Babel.Services.GroupService">
        <endpoint address="" binding="wsHttpBinding" bindingNamespace="http://babylon.com/services" contract="Babel.Services.IGroupService"/>
        <endpoint address="basic" binding="basicHttpBinding" bindingNamespace="http://babylon.com/services" contract="Babel.Services.IGroupService"/>
        <endpoint address="mex" binding="mexHttpBinding" bindingNamespace="http://babylon.com/services" contract="IMetadataExchange"/>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/Babel.Service.Group"/>
          </baseAddresses>
        </host>
      </service>
      <service behaviorConfiguration="BabelServiceBehavior" name="Babel.Services.MediaService">
        <endpoint address="" binding="wsHttpBinding" bindingNamespace="http://babylon.com/services" contract="Babel.Services.IMediaService"/>
        <endpoint address="basic" binding="basicHttpBinding" bindingNamespace="http://babylon.com/services" contract="Babel.Services.IMediaService"/>
        <endpoint address="mex" binding="mexHttpBinding" bindingNamespace="http://babylon.com/services" contract="IMetadataExchange"/>
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/Babel.Service.Media"/>
          </baseAddresses>
        </host>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="BabelServiceBehavior">
          <serviceMetadata httpGetEnabled="true"/>
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
